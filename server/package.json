{
  "name": "sorteio-feliz",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "npm run prisma:deploy && node dist/src/server.js",
    "build": "tsc -p tsconfig.json",
    "dev": "NODE_ENV=development nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register ./src/server.ts' -e ts",
    "test": "npm run env:test npm run prisma:migrate && npm run env:test cross-env NODE_OPTIONS=--experimental-vm-modules jest -- -i",
    "env:dev": "dotenv -e .env.dev",
    "env:test": "dotenv -e .env.test",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:studio": "npx  prisma studio",
    "prisma:seed": "npx prisma db seed",
    "dev:migrate:run": "npm run env:dev npm run prisma:deploy",
    "dev:migrate:generate": "npm run env:dev npm run prisma:migrate",
    "dev:postgres:up": "npm run docker:compose:dev -- up -d drivenops_postgres_dev",
    "dev:postgres:down": "npm run docker:compose:dev -- down",
    "dev:prisma:studio": "npm run env:dev npm run prisma:studio",
    "dev:docker": "npm run docker:compose:dev -- up",
    "docker:dockerfile": "npm run prisma:migrate && npm start",
    "docker:test": "docker-compose -f docker-compose.test.yml",
    "docker:compose": "docker-compose -f docker-compose.yml",
    "docker:compose:dev": "docker-compose -f docker-compose.dev.yml"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "prisma": "^4.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.3",
    "@types/supertest": "^2.0.12",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^6.0.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
